# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-role-key

# WhatsApp Integration (Twilio)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# Currency API
UNIRATE_API_KEY=your-currency-api-key

# Agent Scheduler Configuration
AGENT_SCHEDULER_SECRET=your-secret-for-cron-jobs
CRON_SECRET=your-cron-secret

# Optional: Background Job Configuration
BACKGROUND_JOBS_ENABLED=true
AGENT_EXECUTION_TIMEOUT=300000 # 5 minutes in milliseconds

# Cursor Agents Configuration
CURSOR_AGENT_MODE=development
CURSOR_AGENT_LOG_LEVEL=info
CURSOR_AGENT_CHECKPOINT_DIR=.cursor/checkpoints
CURSOR_AGENT_MAX_CONCURRENT=3

# Document Processing API (External Service)
DOCUMENT_API_URL=https://your-document-api.ngrok-free.app
DOCUMENT_API_EMAIL=your_email@example.com
DOCUMENT_API_PASSWORD=your_password